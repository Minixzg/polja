#include<stdio.h>
main()
{
	int  i, a[50], des[50], jed[50],n;
	printf(" \n Kolkiko zelis unjeti dvoznamenkastih brojeva(max 50): ");
	scanf("%d", &n);
	for (i = 0; i < n; i++) {
		printf(" \n Unesi %d. dvoznamenkasti broj: ", i + 1);
		scanf("%d", &a[i]);
		des[i] = a[i] / 10;
		jed[i] = a[i] % 10;
	}
	for (i = 0; i < n; i++) {
		if (des[i] +jed[i]>5)
			printf(" \n Zbroj znamenaka  %d. broja (%d) je >5.\n", i + 1,a[i] );

	}

}
